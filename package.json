{
  "name": "@sequencemedia/standard-build-ejs",
  "version": "1.0.0",
  "description": "Standard Build (EJS)",
  "private": true,
  "main": "./app.js",
  "author": {
    "name": "Jonathan Perry for Sequence Media Limited",
    "email": "sequencemedia@sequencemedia.net",
    "url": "http://sequencemedia.net"
  },
  "license": "UNLICENSED",
  "scripts": {
    "start": "cross-env NODE_ENV=production DEBUG=@sequencemedia node app",
    "dev": "cross-env NODE_ENV=development npm run gulp",
    "build": "cross-env NODE_ENV=production npm run gulp -- build",
    "build:html": "npm run gulp -- build:html",
    "watch": "cross-env NODE_ENV=development npm run gulp -- watch",
    "watch:html": "npm run gulp -- watch:html",
    "transform": "cross-env NODE_ENV=production npm run gulp -- transform",
    "lint": "npm run lint:js",
    "lint:fix": "npm run lint:js:fix",
    "lint:js": "eslint .",
    "lint:js:fix": "npm run lint:js -- --fix",
    "test": "cross-env NODE_ENV=test mocha test --recursive -r @babel/register --verbose --color",
    "gulp": "gulp --color ",
    "pre-commit": "npm run lint && npm run test && npm run build && npm run transform && git add public"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "debug": "^4.3.2",
    "del": "^6.0.0",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "glob-all": "^3.2.1",
    "gulp": "^4.0.2",
    "gulp-debug": "^4.0.0",
    "gulp-rename": "^2.0.0",
    "mocha": "^9.1.1",
    "module-alias": "^2.2.2",
    "proxyquire": "^2.1.3",
    "sinon": "^11.1.2",
    "sinon-chai": "^3.7.0",
    "vinyl-paths": "3.0.1",
    "yargs-parser": "^20.2.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.4",
    "@babel/eslint-parser": "^7.15.4",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-export-default-from": "^7.14.5",
    "@babel/plugin-proposal-export-namespace-from": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.4",
    "@babel/register": "^7.15.3",
    "babel-plugin-minify-dead-code-elimination": "^0.5.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "chai": "^4.3.4",
    "core-js": "^3.17.2",
    "eslint": "^7.32.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-mocha": "^9.0.0",
    "husky": "^7.0.2",
    "standard": "^16.0.3"
  },
  "_moduleAliases": {
    "~": "."
  }
}
